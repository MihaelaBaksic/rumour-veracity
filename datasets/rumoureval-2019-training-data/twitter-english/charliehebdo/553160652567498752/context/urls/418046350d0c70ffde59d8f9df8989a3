var amp;
var fwGetKeyValuesObj = pc.fwGetKeyValues(abcnws_fw_params.keyValues);
akamai.amp.AMP.loadDefaults("/assets/player/amp/2.0.0012/_config/default.xml?v=3");
function loadHandler()
{
var config_overrides =
{
autoplay: false,
