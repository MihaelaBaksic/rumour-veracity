<script type="text/javascript">abcnws_fw_params = {siteSectionId: 'nws_international', siteSectionIdType: 0, siteSectionNetworkId: 168234, siteSectionFallbackId: 109523, customVisitor:'', keyValues:'pageType=story'}; pc.fwSeg();pc.fwAppendKeyValues('show=wn');pc.getSWID();pc.subsectionOverride();
