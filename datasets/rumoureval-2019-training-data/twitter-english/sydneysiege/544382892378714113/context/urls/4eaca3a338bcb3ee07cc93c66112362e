	BF_DFP_SITE='bfd.bpage';
}
BF_TAG = '';
BF_DFP_ZONE='/australia';
if (typeof BF_DFP_ORD == 'undefined') {BF_DFP_ORD=Math.random()*100000000000000000;}
BF_DFP_KVS='bid=3546170;badge=trending;user=bradesposito;nsfw=0;hot=1';
BF_NO_QS = true;
BF_DFP_NOADS={};

var is_referer = "";

if(is_referer){
	BF_REFERER = is_referer;
}
else if(document.referrer && !is_referer){
	BF_REFERER = document.referrer;
	if(BF_REFERER.search('buzzfeed.com') != -1) BF_REFERER = 'buzzfeed';
}
else{
	BF_REFERER = 'undefined';
}
BF_REFERER = BF_REFERER.replace('http://','').replace('https://','').replace('www.','').split(/[/?#]/)[0];
if(BF_REFERER.match('\.')) BF_REFERER = BF_REFERER.replace(/\./g, '_');
else BF_REFERER = BF_REFERER;


var AB_AD_TEST = false;

	AB_AD_TEST = true;

var AB_RND_VAL = Math.floor((Math.random() * 100) + 1);

if (AB_AD_TEST) {
	// bpage test
	
	 // 0% GPT b-page until ad refresh refactor
	// homepages test (includes metaverticals)
	
	
	
		 // default to 0% gpt
	

	var AD_DESIGN;
	if (AB_RND_VAL >= 0 && ( AB_RND_VAL < 100 || AB_RND_VAL < 100 )) {
		AD_DESIGN = 'script';
	}
	else {
		AD_DESIGN = 'gpt';
	}
	
	if (window['localStorage'] !== null) {
		if (localStorage.getItem("GPT_BP3") == null) {
			localStorage.setItem("GPT_BP3", AD_DESIGN);
		}
		if (localStorage.getItem("") == null) {
			localStorage.setItem("", AD_DESIGN);
		}
		
		AD_DESIGN = localStorage.getItem("GPT_BP3");
		
	}

	// Add to ab_test.js
	(function() {
		if (AB_AD_TEST && AD_DESIGN) {
			if (typeof abtest != 'undefined' && (!abtest.abTests.match(/GPT_BP3|/) )) {
				abtest.addTest("GPT_BP3", AD_DESIGN);
				abtest.addTest("", AD_DESIGN);
			} else {
				setTimeout(arguments.callee, 100)
			}
		}
	})();
} else {
	AD_DESIGN = 'script';
	
}






	try{
		if (AB_AD_TEST && AD_DESIGN == 'gpt') {
			
	var gptadslots=[];
	var googletag = googletag || {};
		googletag.cmd = googletag.cmd || [];
	(function(){ var gads = document.createElement('script');
		gads.async = true; gads.type = 'text/javascript';
		var useSSL = 'https:' == document.location.protocol;
		gads.src = (useSSL ? 'https:' : 'http:') + '//www.googletagservices.com/tag/js/gpt.js';
		var node = document.getElementsByTagName('script')[0];
		node.parentNode.insertBefore(gads, node);
	})();

	function loadGPTAd(params) {
		if (!params || !params['wid']) return false;

		googletag.cmd.push(function(){
			if (params.infinite_scroll) {
				gptadslots[params.wid] = googletag.defineSlot('/6556/' + BF_DFP_SITE + BF_DFP_ZONE, [[5,5]],'div-gpt-ad-' + params.wid)
					.setTargeting('wid', params.wid)
					.setTargeting('pos',['story' + params.wid])
					.addService(googletag.pubads());
			}
			googletag.display('div-gpt-ad-' + params.wid);
			googletag.pubads().refresh( [gptadslots[params.wid]] );
		});
	}

	googletag.cmd.push(function() {
		
			
				gptadslots[13]= googletag.defineSlot('/6556/' + BF_DFP_SITE + BF_DFP_ZONE, [[300,250]],'div-gpt-ad-13')
