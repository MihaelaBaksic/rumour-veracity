        <script type="text/javascript">
        
            DM.later('bundle', function(){
                DM.has('socialLinks', 'SCExternalLinks', {
                    value: 'share 2873855'
                });
            });
        
            // Google +1 callback used in article_icon_links.jsp
            window.plusone_vote = function(obj) {
                DM.later('bundle', function(){
                    DM.pageEvents.fireEvent(DM.pageEvents.SOCIAL_BUTTON_CLICKED, {
                        pageMetadata: DM.getPageMetadata(),
                        type: 'plusone-vote',
                        channel: 'news',
                        link: obj.href
                    });
        
                    DM.ComScore.connectCallback('plusone-vote', obj);
                });
            };
        
            window.twitterCallback = function(intent_event) {
                DM.later('bundle', function(){
                    var placement, $button = $(intent_event.target);
                    // if I get a placement it is a follow button
                    if ($button.is('.twitter-follow-author')){
                        placement = 'author';
                    }
                    else if ($button.is('.twitter-follow-header')){
                        placement = 'header';
                    }
        
                    if (placement) {
                        DM.pageEvents.fireEvent(DM.pageEvents.SOCIAL_SHARE_CLICKED, {
                            pageMetadata: DM.getPageMetadata(),
                            site: 'twitter-follow',
                            placement: placement
                        });
                    }
                    else {
                        DM.pageEvents.fireEvent(DM.pageEvents.SOCIAL_BUTTON_CLICKED, {
                            pageMetadata: DM.getPageMetadata(),
                            type: 'twitter-click',
                            channel: 'news',
                            link: intent_event.target.baseURI
                        });
                    }
                    DM.ComScore.connectCallback('twitter-click', intent_event);
                });
            };
        
            window.___gcfg = {lang: 'en-GB'};
            window.articleShortUrl = '/web/20141215065948/http://dailym.ai/134wluW';
        
            (function() {
                var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true;
                po.src = '/web/20141215065948/https://apis.google.com/js/plusone.js';
                var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s);
            })();
        
            (function(d, s, id) {
                var js, fjs = d.getElementsByTagName(s)[0];
                if (d.getElementById(id)) return;
                js = d.createElement(s); js.id = id;
                js.src = '//connect.facebook.net/en_GB/all.js#xfbml=1&appId=146202712090395&channelUrl=/web/20141215065948/http://www.dailymail.co.uk/facebook/channel.html&status=1&cookie=1&oauth=1';
                fjs.parentNode.insertBefore(js, fjs);
            }(document, 'script', 'facebook-jssdk'));
        
            if (!window.fbAsyncInit) {
                window.fbAsyncInit = function() {
                    FB.Event.subscribe('edge.create', function(response) {
                        DM.later('bundle', function(){
                            DM.ComScore.connectCallback('facebook-like', response);
                            DM.pageEvents.fireEvent(DM.pageEvents.SOCIAL_BUTTON_CLICKED, {
                                pageMetadata: DM.getPageMetadata(),
                                type: 'facebook-like',
                                channel: 'news',
                                link: '/web/20141215065948/http://www.dailymail.co.uk/news/article-2873855/Gunman-takes-hostages-cafe-Sydney.html'
                            });
                        });
                    });
                }
            }
        
            if (!window.twttr) {
                window.twttr = (function (d,s,id) {
                    var t, js, fjs = d.getElementsByTagName(s)[0];
                    if (d.getElementById(id)) return; js=d.createElement(s); js.id=id;
                    js.src='//platform.twitter.com/widgets.js'; fjs.parentNode.insertBefore(js, fjs);
                    return window.twttr || (t = { _e: [], ready: function(f){ t._e.push(f) } });
                }(document, 'script', 'twitter-wjs'));
