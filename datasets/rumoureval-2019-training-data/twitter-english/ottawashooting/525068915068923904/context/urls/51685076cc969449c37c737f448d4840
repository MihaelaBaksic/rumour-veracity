		}

		jQuery('.flexslider').flexslider({
			slideshow: true,
			slideshowSpeed: 7000,
			video: true,
			smoothHeight: flex_smoothHeight,
			pauseOnHover: false,
			useCSS: false,
			start: function(slider) {
